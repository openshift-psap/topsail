---
apiVersion: v1
kind: ConfigMap
metadata:
  name: {{ gpu_operator_enable_time_sharing_configmap_name }}
  namespace: {{ gpu_operator_enable_time_sharing_namespace }}
data:
  any: |-
    version: v1
    flags:
      migStrategy: none
    sharing:
      timeSlicing:
        resources:
        - name: nvidia.com/gpu
          replicas: {{ gpu_operator_enable_time_sharing_replicas }}
